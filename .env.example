# API Keys
MISTRAL_API_KEY=your_mistral_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud - Local Development Only
# For local dev: Point to your service account JSON file
# For Cloud Run: Leave unset to use Application Default Credentials
GOOGLE_APPLICATION_CREDENTIALS=credentials.json
GCS_BUCKET_NAME=your-gcs-bucket-name
GCS_PROJECT_ID=your-gcp-project-id

# Google Sheets
MASTER_SHEET_ID=your-master-sheet-id-here
OUTPUT_SHEET_ID=your-output-sheet-id-here

# Optional Configuration
FUZZY_MATCH_THRESHOLD=0.75
MAX_UPLOAD_SIZE_MB=10
CACHE_TTL_SECONDS=300
CONFIDENCE_WARNING=0.7
OCR_CONCURRENCY=2
